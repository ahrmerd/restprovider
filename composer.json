{
    "name": "ahrmerd/restprovider",
    "description": "provide commands to easily develop rest api using laravel PHP framework",
    "require": {
        "php": "^8.2",
        "spatie/laravel-query-builder": "^5.1"
    },
    "require-dev": {
        "orchestra/testbench": "^7.18"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ahrmerd\\RestProvider\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ahrmerd\\RestProvider\\RestProviderServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Ahrmerd",
            "email": "Ahrmerd@gmail.com"
        }
    ]
}
